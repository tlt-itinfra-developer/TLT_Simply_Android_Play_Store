<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/unactive"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="1"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_4"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_2"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="2"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_5"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_3"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="3"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="4"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_7"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_1"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="5"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_8"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_6"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_4"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_2"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="6"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_5"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_3"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="7"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/TLTButton"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_8"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_4"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="8"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_0"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_9"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_7"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_5"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="9"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toTopOf="@+id/pin_number_del"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_8"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_6"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/TLTButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:clickable="false"
        android:fontFamily="@font/custom_bold"
        android:gravity="center"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_0"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_7"
        app:layout_constraintVertical_weight="1" />

    <tlt.th.co.toyotaleasing.view.TLTButton
        android:id="@+id/pin_number_0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/pincode_number"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pin_number_del"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/TLTButton"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_8"
        app:layout_constraintVertical_weight="1" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/pin_number_del"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/keyboard_pincode_spacing"
        android:background="?selectableItemBackgroundBorderless"
        android:fontFamily="@font/custom_regular"
        android:scaleType="center"
        android:textSize="@dimen/pincode_number_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pin_number_0"
        app:layout_constraintTop_toBottomOf="@+id/pin_number_9"
        app:layout_constraintVertical_weight="1"
        app:srcCompat="@drawable/ic_clear" />
</androidx.constraintlayout.widget.ConstraintLayout>